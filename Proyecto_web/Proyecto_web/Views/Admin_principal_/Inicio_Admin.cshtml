
@{
    ViewBag.Title = "Inicio_Admin";
    Layout = "~/Views/Admin_principal_/Admin_Principal.cshtml";
}
@if (Session["Alerta"].ToString() == "1")
{
    <script type="text/javascript">
            window.onload = function () {
                swal('Felicidades','Ha iniciado sesion','success')
            }
    </script>
    Session["Alerta"] = "2";
}

<div id="overlay">
    <div class="spinner"></div>
</div>
<h1>Bienvenido Administrador</h1>

<link href="~/Content/Load.css" rel="stylesheet" />

<div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
    <div class="tile-stats">
        <div class="icon"><i class="fa fa-child"></i></div>
        <div class="count">1</div>
        <h3><a href="@Url.Action("Perfil_Admin","Admin_principal_")">Mi Perfil </a></h3>
        <p >Ver Más.</p>
    </div>
</div>


<div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
